  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Api",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "./app/api/main.go",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Tracker",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "./app/tracker/main.go",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Trading Bot",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "./app/trading-bot/main.go",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Launch Current File",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${file}",
      "cwd": "${workspaceFolder}"
    }
  ]
}
